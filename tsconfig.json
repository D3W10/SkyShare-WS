{
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "target": "ES2024",
        "module": "ESNext",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "forceConsistentCasingInFileNames": true
    }
}